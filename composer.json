{
    "name": "arokettu/datetime-rounding",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\DateTime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\DateTime\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.1 | ^8.0"
    },
    "require-dev": {
        "nesbot/carbon": "^2.71",
        "phpunit/phpunit": ">= 7.5 < 10",
        "sandfox.dev/code-standard": "^1.2023.06.29",
        "squizlabs/php_codesniffer": "*"
    }
}
